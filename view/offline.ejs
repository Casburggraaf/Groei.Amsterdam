<% include head.ejs %>
<body class="no-javscript">
  <img src="/style/loader.svg" alt="loaderGif">
  <nav class="slidecontainer">
    <input type="range" min="1850" max="2018" value="1900" class="slider" id="myRange">
    <span id="play" class="buttons">
      <i class="fas fa-play buttons" data-fa-transform="grow-6"></i>
      <i class="fas fa-pause buttons hid" data-fa-transform="grow-6"></i>
    </span>
    <span id="forward" class="buttons"><i class="fas fa-angle-double-right buttons" data-fa-transform="grow-6"></i></span>
    <p><span id="demo"></span></p>
  </nav>
  <div id="map"></div>
  <div class="overZicht">
    <h1>U bent offline. Voor een complete werkende website is een internet verbinding verplicht</h1>
    <h2>Straten</h2>
    <ul id="straten">
      <% Object.keys(renderData).forEach(function(key) {%>
        <li>
          <h3 id="jaar-<%=key%>"><%=key%>, <%=renderData[key].length%> nieuwe straten.</h3>
          <ul>
            <%renderData[key].forEach(function(el){%>
              <li><a href="<%-el.url%>"><%=el.naam%></a></li>
            <%});%>
          </ul>
        </li>
      <%});%>
    </ul>
  </div>
</body>
<% include tail.ejs %>
